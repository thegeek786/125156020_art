{
    "version": 2,
    "builds": [
      {
        "src": "./index.py",
        "use": "@vercel/python",
        "config": {
          "runtime": "python3.8"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],
    "buildCommand": "./install-portaudio.sh && vercel-build"
  }
  
